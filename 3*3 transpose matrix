#include<iostream>
using namespace std;
int main(){
int matrix[3][3];
cout<<"Enter elements of Matrix:"<<endl;
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        cin>>matrix[i][j];
    }
}
cout<<"The Matrix is:"<<endl;
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        cout<<matrix[i][j]<<" ";
    }
    cout<<endl;
}
int tranpose[3][3];
cout<<"The Transpose Matrix is:"<<endl;
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        cout<<matrix[j][i]<<" ";
    }
    cout<<endl;
}
return 0;
}
